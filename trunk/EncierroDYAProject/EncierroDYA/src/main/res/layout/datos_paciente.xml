<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">



        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="left">

            <ImageButton
                android:id="@+id/home_menu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@string/lbl_home"
                android:padding="@dimen/padding_medium"
                android:src="@drawable/home"
                android:background="@null"
                android:scaleType="fitEnd"/>

            <TextView
                android:id="@+id/lbl_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/padding_medium"
                android:text="@string/lbl_home"
                android:textStyle="bold"/>
        </LinearLayout>




    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/txtStatus"
        android:id="@+id/button"
        android:layout_gravity="center"/>


    <LinearLayout android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout   android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imgTriage"
                android:layout_width="30dip"
                android:layout_height="30dip"
                android:layout_gravity="center"
                android:contentDescription="@string/triage"/>
            <TextView
                android:id="@+id/blank"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dip"/>
            <TextView
                android:id="@+id/iid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:textStyle="bold"
                android:background="#cecacf"
                android:padding="10dip"/>
            <TextView
                android:id="@+id/blank1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="4dip"/>

            <ImageView
                android:id="@+id/imgConscious"
                android:layout_width="10dip"
                android:layout_height="10dip"
                android:layout_gravity="center"
                android:contentDescription="@string/muyGrave"/>
            <TextView android:id="@+id/txtConscious"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="6dip"
                android:paddingRight="10dip"
                android:layout_gravity="center"/>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="right">
                <TextView
                    android:id="@+id/ambulance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    android:background="#cecacf"
                    android:padding="10dip"
                    android:visibility="gone"
                    android:layout_gravity="center"/>
                <TextView
                    android:id="@+id/blank2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="6dip"
                    android:paddingBottom="6dip"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"/>
                <TextView
                    android:id="@+id/health_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:background="#cecacf"
                    android:padding="10dip"
                    android:visibility="gone"
                    android:layout_gravity="center"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:orientation="horizontal"
                android:layout_gravity="left">

                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gender"
                    android:paddingLeft="6dip"
                    android:paddingTop="10dip"
                    android:textSize="17sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/gender"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="6dip"
                    android:paddingRight="6dip"
                    android:textSize="17sp"
                    android:textStyle="normal" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/old"
                    android:paddingLeft="6dip"
                    android:paddingTop="10dip"
                    android:textSize="17sp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/old"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="6dip"
                    android:paddingRight="6dip"
                    android:textSize="17sp"
                    android:textStyle="normal"
                    android:layout_gravity="center"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="right">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/stand"
                    android:paddingLeft="6dip"
                    android:paddingTop="10dip"
                    android:textSize="17sp"
                    android:textStyle="bold|italic"
                    android:layout_gravity="right"/>

                <TextView
                    android:id="@+id/stand"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="6dip"
                    android:paddingRight="6dip"
                    android:textSize="17sp"
                    android:textStyle="normal|italic"
                    android:layout_gravity="center"/>
            </LinearLayout>
        </LinearLayout>






        <LinearLayout   android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">





            <LinearLayout android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip">
                <LinearLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip">
                    <TextView android:id="@+id/txtSymtoms"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold|italic"
                        android:textSize="15sp"
                        android:text="@string/symptom"
                        android:paddingRight="10dip" />

                </LinearLayout>
                <LinearLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgGoringHead"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtGoringHead"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgGoringChest"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtGoringChest"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgGoringBack"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtGoringBack"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgGoringArms"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtGoringArms"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgGoringLegs"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtGoringLegs"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgTce"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtTce"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgHemorrhages"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtHemorrhages"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgPenetratingInjuryTx"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtPenetratingInjuryTx"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgPenetratingInjuryAbdomen"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/red"
                            android:contentDescription="@string/muyGrave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtPenetratingInjuryAbdomen"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgBreakHead"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/yellow"
                            android:contentDescription="@string/grave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtBreakHead"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgBreakChest"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/yellow"
                            android:contentDescription="@string/grave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtBreakChest"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgBreakLegs"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/yellow"
                            android:contentDescription="@string/grave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtBreakLegs"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgBreakArms"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/yellow"
                            android:contentDescription="@string/grave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtBreakArms"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgPolicontusion"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/yellow"
                            android:contentDescription="@string/grave"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtPolicontusion"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <ImageView
                            android:id="@+id/imgBruises"
                            android:layout_width="8dip"
                            android:layout_height="8dip"
                            android:src="@drawable/green"
                            android:contentDescription="@string/leve"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtBruises"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:layout_gravity="center"/>
                    </LinearLayout>

                    <TextView android:id="@+id/txtFace"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="11sp"
                        android:paddingLeft="9dip"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dip"
                android:paddingRight="10dip">
                <LinearLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip">
                    <TextView android:id="@+id/txtAdvanced"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold|italic"
                        android:textSize="15sp"
                        android:text="@string/advanced"
                        android:paddingRight="10dip"/>

                </LinearLayout>
                <LinearLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">

                        <TextView android:id="@+id/lblGlasgow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:visibility="gone"
                            android:text="@string/glasgow"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtGlasgow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <TextView android:id="@+id/lblBloodPresure"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:text="@string/bloodPresure"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtBloodPresure"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip">
                        <TextView android:id="@+id/lblTrachealIntubation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:text="@string/trachealIntubation"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                        <TextView android:id="@+id/txtTrachealIntubation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:visibility="gone"
                            android:layout_gravity="center"/>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
</LinearLayout>